    "react-dom": "^19.2.3",
    "react-icons": "^5.5.0",
    "react-phone-number-input": "^3.4.14",
    "react-router-dom": "^6.30.3",
    "react-scripts": "5.0.1",
    "react-simple-keyboard": "^3.8.141",
